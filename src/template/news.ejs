<div class="page-main">
    <div class="page-banner">
        <div class="container">
            <div class="page-title">
                新闻资讯
            </div>
            <div class="page-breadcrumb">
                <a href="/">首页</a>
                <span>/</span>
                <em>新闻资讯</em>
            </div>
        </div>
    </div>
    <div class="page-news-contaienr container">
        <div class="news-content clearfix">
            <% for(var i=0;i<data.length;i++)   {%>
                <div class="news-item <%=  (i-1)%3==0? 'center':''  %>  ">
                    <div class="news-item-container">
                        <div class="pic">
                            <a href="/news/<%= data[i].id %>">
                                <img src="<%= data[i].url %>" alt="" class="imgs">
                            </a>
                        </div>
                        <div class="content-container">
                            <h1 class="title">
                                <a href="/news/<%= data[i].id %>">
                                    <%=  data[i].title  %>
                                </a>
                            </h1>
                            <p class="content">
                                <%= data[i].abstract  %>
                            </p>
                            <div class="clearfix other">
                                <p class="label lf">
                                    <%= data[i].category  %>
                                </p>
                                <p class="time rt">
                                        <%= filterTimeYMD(data[i].create_time)  %>
                                    </p>
                            </div>
                        </div>
                    </div>
                </div>
                <% } %>
        </div>
    </div>
</div>