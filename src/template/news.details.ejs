<div class="page-main">
    <div class="page-banner">
        <div class="container">
            <div class="page-title">
                新闻资讯
            </div>
            <div class="page-breadcrumb">
                <a href="/">首页</a>
                <span>/</span>
                <a href="/news">新闻资讯</a>
                <span>/</span>
                <em>资讯详情</em>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="news-details-container">
            <div class="news-details-header">
                <h1 class="title">
                    <%= title %>
                </h1>
                <div class="info">
                    <p class="label">
                        <%= category %>
                    </p>
                    <p class="time">
                            <%= filterTimeYMD(create_time)  %>
                        </p>
                </div>
            </div>
            <div class="news-details-content ">
                <%= content%>
            </div>
        </div>
    </div>
</div>